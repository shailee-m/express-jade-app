extend layout


block content 
    div.container
        h1.justify-content-center Table of Contents
        table.table.table-striped.table-bordered.table-hover
            thead
                tr
                    -var keys = Object.keys(details[0])
                    -for( i=0;i<keys.length;i++ ){
                        th.active= keys[i]                    
                    - }
            tbody
                - var iters = details.length
                -for(i=0;i<iters;i++){
                    tr
                    -var keys = Object.keys(details[0])
                    -for( j=0;j<keys.length;j++ ){
                        - if(i%3==0)
                            th.success= details[i][keys[j]] 
                        - else
                            th= details[i][keys[j]]                    
                    - } 
                - }
                
